{
    "name": "concretehouse/classes-component",
    "description": "Experimental implementation of components related to class-management.",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "tamakiii",
            "email": "tamaki.htm@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Concretehouse\\Component\\Classes\\": "src"
        }
    },
    "require": {
        "php": ">=5.3.2",
        "pimple/pimple": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
